stop();

else;
log("&f[&bMACRO&f] &6SupFISHING Foi &aIniciado&6!.");
toggle(supfishing);
endif;
}$$

/home pesqueiro

$${
gui(Inventory);  
pick(346);
look(,0);
look(,-90);
do();
  
if(%DURABILITY%<10);
log("&f[&bMACRO&f] &aREPARANDO&6!.");
gui();
look(,90);
wait(1);
key(use);
wait(1);
key(use);
wait(1);
look(,0);
look(,-90);
}$$

/skills

$${
gui(Inventory);
endif;
key(use);
wait(50ms);
loop;
}$$

