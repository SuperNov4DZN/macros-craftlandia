                WAIT(40T);
                LOG("");
                LOG("&e[Info] &fLogin realizado. &9&o#SLEEP");
                LOG("");
            ENDIF;
    ENDIF;
    IFMATCHES("%CHATCLEAN%","^(.*) deseja ir até você! Use \/bring \@nick para aceitar.$",&nick_bring,1,NONE);
        LOG("");
